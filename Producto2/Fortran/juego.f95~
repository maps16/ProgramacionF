program juego

  write(*,*) 'Hola, es hora de jugar ADIVINA EL NUMERO. Primero piensaen un numero entre 1 y 10'
  
  call sleep(5)

  Write(*,*) 'Multiplicalo por 9'

  call sleep(5)

  write(*,*) 'Si el numero es de dos digitos, sumalos entre si. Por ejemplo si es 25 -> 2+5=7'

  call sleep(5)

  write(*,*) 'El numero resultante sumale 4'

  call sleep(10)

  write(*,*) 'Bien. El resultado es 13'

end program juego
